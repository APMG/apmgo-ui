{
  "passPerPreset": true,
  "presets": [
    { "plugins": [ "transform-runtime" ] },
    {
        "passPerPreset": false,
        "presets": [
          [ "es2015", { "modules": false } ],
          "stage-1",
          "flow",
          "react"
        ]
    }
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "transform-flow-strip-types",
        "transform-regenerator"
      ]
    }
  }
}
